!
! Swap Caps_Lock and Escape
!

remove Lock = Caps_Lock
keysym Escape = Caps_Lock
keysym Caps_Lock = Escape
add Lock = Caps_Lock


!
! Make Page Down and Page Up End and Home
!

keysym Prior = Home
keysym Home = Prior
keysym Next = End
keysym End = Next

!
! Make Right Shift ISO_Level3_Shift
!
remove Shift = Shift_R
keycode 62 = ISO_Level3_Shift
	
