[user]
	email = t.perrier@gmail.com
	name = Trevor
[push]
	default = simple
[alias]
	hist = "log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
	head = "!git --no-pager log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --max-count=10;echo"
[core]
	editor = vim

[color]
	branch = auto
	diff = auto
	status = auto
